st) popular <- get_popular_palettes() plot(popular)"},{"path":"/index.html","id":"ggplot2-scales","dir":"","previous_headings":"Example","what":"ggplot2 scales","title":"Colors and Palettes from Color-Hex","text":"","code":"library(ggplot2)  ggplot(mtcars, aes(mpg)) +    geom_density(aes(fill = disp, group = disp)) +    scale_fill_palettehex_c(popular) ggplot(mtcars, aes(mpg)) +   geom_density(aes(fill = disp, group = disp)) +   scale_fill_palettehex_c(popular, 3) ggplot(mtcars, aes(mpg, disp, colour = factor(cyl))) +    geom_point() +    scale_color_palettehex_d(popular) ggplot(mtcars, aes(mpg, disp, colour = factor(cyl))) +    geom_point() +    scale_color_palettehex_d(popular, 1872) x <- get_color(\"#008080\")  ggplot(mtcars, aes(mpg)) +    geom_density(aes(fill = disp, group = disp)) +    scale_fill_colorhex_c(x) ggplot(mtcars, aes(mpg)) +   geom_density(aes(fill = disp, group = disp)) +   scale_fill_colorhex_c(x, \"tints\") ggplot(mtcars, aes(mpg)) +   geom_density(aes(fill = disp, group = disp)) +   scale_fill_colorhex_c(x, \"shades\") ggplot(mtcars, aes(mpg, disp, colour = factor(cyl))) +    geom_point() +    scale_color_colorhex_d(x, \"triadic\") ggplot(mtcars, aes(mpg, disp, colour = factor(cyl))) +    geom_point() +    scale_color_colorhex_d(x, \"shades\")"},{"path":"/reference/get_color.html","id":null,"dir":"Reference","previous_headings":"","what":"Get color information — get_color","title":"Get color information — get_color","text":"Get color information www.color-hex.com hex-color.","code":""},{"path":"/reference/get_color.html","id":"ref-usage","dir":"Reference","previous_headings":"","what":"Usage","title":"Get color information — get_color","text":"","code":"get_color(hex)"},{"path":"/reference/get_color.html","id":"arguments","dir":"Reference","previous_headings":"","what":"Arguments","title":"Get color information — get_color","text":"hex character string hexidecimal color","code":""},{"path":"/reference/get_color.html","id":"value","dir":"Refere